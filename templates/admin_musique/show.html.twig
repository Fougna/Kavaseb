{% extends 'base.html.twig' %}

{% block title %}Musique{% endblock %}

{% block body %}
    <h1>Musique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ musique.id }}</td>
            </tr>
            <tr>
                <th>TitreOriginal</th>
                <td>{{ musique.titreOriginal }}</td>
            </tr>
            <tr>
                <th>SurTitreOriginal</th>
                <td>{{ musique.surTitreOriginal }}</td>
            </tr>
            <tr>
                <th>SousTitreOriginal</th>
                <td>{{ musique.sousTitreOriginal }}</td>
            </tr>
            <tr>
                <th>TitreFrancais</th>
                <td>{{ musique.titreFrancais }}</td>
            </tr>
            <tr>
                <th>SurTitreFrancais</th>
                <td>{{ musique.surTitreFrancais }}</td>
            </tr>
            <tr>
                <th>SousTitreFrancais</th>
                <td>{{ musique.sousTitreFrancais }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ musique.image }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ musique.annee ? musique.annee|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Duree</th>
                <td>{{ musique.duree }}</td>
            </tr>
            <tr>
                <th>Genre</th>
                <td>{{ musique.genre }}</td>
            </tr>
            <tr>
                <th>Chronologie</th>
                <td>{{ musique.chronologie }}</td>
            </tr>
            <tr>
                <th>NoteMusique</th>
                <td>{{ musique.noteMusique }}</td>
            </tr>
            <tr>
                <th>Art</th>
                <td>{{ musique.art }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_musique_index') }}">back to list</a>

    <a href="{{ path('admin_musique_edit', {'id': musique.id}) }}">edit</a>

    {{ include('admin_musique/_delete_form.html.twig') }}
{% endblock %}
