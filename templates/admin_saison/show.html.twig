{% extends 'base.html.twig' %}

{% block title %}Saison{% endblock %}

{% block body %}
    <h1>Saison</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ saison.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ saison.titre }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ saison.annee ? saison.annee|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ saison.numero }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_saison_index') }}">back to list</a>

    <a href="{{ path('admin_saison_edit', {'id': saison.id}) }}">edit</a>

    {{ include('admin_saison/_delete_form.html.twig') }}
{% endblock %}
